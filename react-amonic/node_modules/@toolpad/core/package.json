{
  "name": "@toolpad/core",
  "version": "0.6.0",
  "author": "Toolpad Team",
  "description": "Dashboard framework powered by MUI.",
  "main": "./node/index.js",
  "module": "./index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./node/index.js"
    },
    "./*": {
      "types": "./*/index.d.ts",
      "import": "./*/index.js",
      "require": "./node/*/index.js"
    }
  },
  "keywords": [
    "react",
    "mui",
    "toolpad",
    "internal tools",
    "crud",
    "admin",
    "dashboard"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mui/mui-toolpad.git",
    "directory": "packages/toolpad-core"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mui/mui-toolpad/issues"
  },
  "homepage": "https://github.com/mui/mui-toolpad#readme",
  "dependencies": {
    "@babel/runtime": "^7.25.6",
    "@mui/lab": "6.0.0-beta.9",
    "@mui/utils": "6.1.0",
    "client-only": "^0.0.1",
    "invariant": "2.2.4",
    "path-to-regexp": "6.2.2",
    "prop-types": "15.8.1",
    "@toolpad/utils": "0.6.0"
  },
  "peerDependencies": {
    "@mui/icons-material": "5 - 6",
    "@mui/material": "5 - 6",
    "next": "^14",
    "react": "^18"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "private": false,
  "types": "./index.d.ts"
}